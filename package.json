{
  "name": "baby-emotion-detector",
  "version": "1.0.0",
  "description": "MERN stack web app for detecting baby emotions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "react-app-rewired": "^2.2.1"
  },
  "keywords": [
    "baby",
    "emotion",
    "detection",
    "mern",
    "ai"
  ],
  "author": "Your Name",
  "license": "MIT"
}
