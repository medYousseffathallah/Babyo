{
  "name": "baby-emotion-detector",
  "version": "1.0.0",
  "description": "MERN stack web app for detecting baby emotions",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
